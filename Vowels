import java.util.*;
class Vowels{
	public static void main(String[] args) {
		int v=0;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a string:");
		String s=sc.nextLine();
		int n=s.length();
		for(int i=0;i<n;i++){
			char ch=s.charAt(i);
			if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u'){
				v++;
			}
		}
		System.out.println("The number of vowels in the input string is:"+v);
	}
}
