import java.util.*;
class Factorial{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Input the number who factorial u want to find:");
		int n=sc.nextInt();
		int f=1;
		for(int i=1;i<=n;i++){
			f=f*i;
		}
		System.out.println("Fcatorial of "+n+" is: "+f);
	}
}
